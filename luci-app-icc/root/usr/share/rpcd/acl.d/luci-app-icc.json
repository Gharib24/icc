{
	"luci-app-icc": {
		"description": "Grant access to LuCI app ICC",
		"read": {
			"ubus": {
				"luci.icc": [ "getData", "getLEDs" ]
			},
			"file": {
				"/sbin/logread -e icc": [ "exec" ],
			},
			"uci": [ "icc" ]
		},
		"write": {
			"ubus": {
				"luci.icc": [ "setInitAction" ]
			},
			"uci": [ "icc" ]
			
		}
	}
}











