
config icc
	option hotplug '1'
	option debug '0'
	option panic '60'
	option ifname 'wlan0'
	option host '8.8.8.8'
	option timeout '15'
	option if_startup_d '/etc/icc/startup.d'
	option if_online_d '/etc/icc/online.d'
	option if_offline_d '/etc/icc/offline.d'
	option if_panic_d '/etc/icc/panic.d'
	option interval '0'

